@startuml
'skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false
skinparam linetype polyline
'skinparam linetype orto
skinparam classAttributeIconSize 0
'left to right direction

class Agenda {
    - List<Vehicle> vehicleList
    + requestList(activation)
    - getVehiclesNeedingCheckUp(activation)
    - checkUpListCreated()
    + getVehicleCheckUpList(): List<Vehicle>
}

class Repositories {
    - Repositories()
    + getInstance() : Repositories
    + getVehicleRepository()
}

class AgendaController {
    + AgendaController()

}

class AgendaUI {
}

AgendaUI ..> AgendaController

AgendaController --> AgendaUI : sendVehicleListToUI()
AgendaController .> Repositories : repositories
Repositories  --> "1 " Agenda
Repositories -> Repositories : instance
AgendaController ..>  Agenda : vehicleRepository
Agenda -> AgendaController : getVehicleCheckUpList()
@enduml
