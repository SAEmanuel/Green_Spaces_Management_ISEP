@startuml
'skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false
skinparam linetype polyline
'skinparam linetype orto
skinparam classAttributeIconSize 0
'left to right direction

class AgendaRepository {
    -List<AgendaEntry> agenda
    - getTaskPlannedList()
    - getTaskList()
    - taskPlannedListCreated()
    - taskListCreated()
    + changedTaskStatusList()
    + sortByStartDate()
    + getStatus()
    + requestColabTaskList(): List<AgendaEntry>
    + requestColabPlannedTaskList(): List<AgendaEntry>
}

class Repositories {
    - Repositories()
    + getAgenda()
    + getInstance() : Repositories
}

class AgendaController {
    - agendaRepository: AgendaRepository
    + AgendaController()
    + getResponsible(): String
    + getStatus(): AgendaEntry.Status[]
    + requestColabTaskList(collaborator: Collaborator, startDate: Data, endDate: Data, filterSelection: int): Optional<List<AgendaEntry>>
    + requestPlannedColabTaskList(collaborator: Collaborator, startDate: Data, endDate: Data, filterSelection: int): Optional<List<AgendaEntry>>
    + requestChangedStatusTaskList(collaborator: Collaborator, startDate: Data, endDate: Data, filterSelection: int, confirmation: String, selectedTask: int): Optional<List<AgendaEntry>>
    + requestPossiblePlannedColabTaskList(collaborator: Collaborator, startDate: Data, endDate: Data, filterSelection: int): Optional<List<AgendaEntry>>
}

class RetrieveTaskUI {
}

RetrieveTaskUI ..> AgendaController

AgendaController --> RetrieveTaskUI
AgendaController .> Repositories : repositories
Repositories  --> "1 " AgendaRepository
Repositories -> Repositories : instance
AgendaController ..>  AgendaRepository : agendaRepository
AgendaRepository --> AgendaController : requestColabTaskList()
AgendaRepository --> AgendaController : requestColabPlannedTaskList()
AgendaRepository ---> AgendaController : requestPossibleColabPlannedTaskList()
@enduml
