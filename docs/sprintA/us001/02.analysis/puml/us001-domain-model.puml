@startuml

skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false

title Domain Model - US001

package "HR System" {

    class HRM {
        + register()
    }

    class Skill {
        - name: String
    }

    class ListSkills {
        - skills: List<Skill>
        + addSkill(skill: Skill): void
    }

    HRM "1" - "*" Skill : creates >
    HRM "1" - ListSkills : see >
    Skill "*" -  ListSkills : registered >
}



@enduml

